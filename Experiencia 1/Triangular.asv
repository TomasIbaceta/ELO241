close all;
clear;
clc;

syms x f_1(x);

%variables de usuario
T=50e-6; %periodo
D_porcentaje = 20; %en "%"
def=20; %numero de armonicos
%-----------------------------
D = D_porcentaje/100; %ciclo de trabajo
tau=T*D;

f_1(x)=piecewise(0 < x < tau, (1/tau)*x , tau < x < T , (tau-x)/(T-tau)+1);
fplot(f_1,[-T/5,T+(T/5)]);

a_n=(2/T)*int(f_1(x)* cos((2*n*pi*t)/T),-tau/2,tau/2);
b_n=(2/T)*int(sin((2*n*pi*t)/T),-tau/2,tau/2);


grid on